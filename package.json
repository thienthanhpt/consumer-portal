{
  "name": "consumer-portal",
  "version": "1.1.4",
  "scripts": {
    "ng": "ng",
    "start:sign-up": "ng serve --project sign-up --port 6969",
    "start:my-account": "ng serve --project my-account --port 9696",
    "start:all": "concurrently \"yarn run start:sign-up\" \"yarn run start:my-account\"",
    "build:sign-up": "ng build sign-up",
    "build:my-account": "ng build my-account",
    "build.prod:sign-up": "ng build sign-up --configuration production",
    "build.prod:my-account": "ng build my-account --configuration production",
    "build.prod:all": "yarn run build.prod:sign-up && yarn run build.prod:my-account",
    "build.stage:sign-up": "ng build sign-up --configuration staging",
    "build.stage:my-account": "ng build my-account --configuration staging",
    "build.stage:all": "yarn run build.stage:sign-up && yarn run build.stage:my-account",
    "task:fetch-image": "node tasks/fetch-images.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.8",
    "@angular/cdk": "^7.0.2",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@ngx-pwa/local-storage": "6",
    "@types/yargs": "^12.0.0",
    "bootstrap": "^4.1.3",
    "chart.js": "^2.7.3",
    "concurrently": "^4.0.1",
    "core-js": "^2.5.4",
    "cron": "^1.4.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "ng-recaptcha": "^4.0.0",
    "ng2-file-upload": "^1.3.0",
    "ng2-pdf-viewer": "^5.2.0",
    "ngx-bootstrap": "^3.0.1",
    "popper.js": "^1.14.4",
    "rxjs": "^6.3.2",
    "yargs": "^12.0.2",
    "zone.js": "~0.8.26"
